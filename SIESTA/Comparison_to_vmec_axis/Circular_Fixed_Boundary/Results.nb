Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"sortedDirectories", "[", "parent_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Sort", "[", 
     RowBox[{
      RowBox[{"FileNames", "[", 
       RowBox[{"\"\<*_*.*\>\"", ",", "parent"}], "]"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"ToExpression", "[", 
         RowBox[{"Last", "[", 
          RowBox[{"StringSplit", "[", 
           RowBox[{"#1", ",", "\"\<_\>\""}], "]"}], "]"}], "]"}], "<", 
