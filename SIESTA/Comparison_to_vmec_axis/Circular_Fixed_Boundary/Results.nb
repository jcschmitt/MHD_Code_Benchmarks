Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"vmecAxisR", "[", "directory_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"xm", ",", "xn", ",", "rmnc"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xm", "=", 
       RowBox[{"loadVMECQuantity", "[", 
        RowBox[{"directory", ",", "\"\<xm\>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"xn", "=", 
       RowBox[{"loadVMECQuantity", "[", 
        RowBox[{"directory", ",", "\"\<xn\>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"rmnc", "=", 
       RowBox[{"loadVMECQuantity", "[", 
        RowBox[{"directory", ",", "\"\<rmnc\>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Total", "[", 
       RowBox[{
        RowBox[{"rmnc", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "*", 
        RowBox[{"Cos", "[", 
         RowBox[{
          RowBox[{"xm", "*", "0"}], "-", 
          RowBox[{"xn", "*", "0"}]}], "]"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"vmecAxisZ", "[", "directory_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"xm", ",", "xn", ",", "rmnc"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xm", "=", 
       RowBox[{"loadVMECQuantity", "[", 
        RowBox[{"directory", ",", "\"\<xm\>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"xn", "=", 
       RowBox[{"loadVMECQuantity", "[", 
        RowBox[{"directory", ",", "\"\<xn\>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"rmnc", "=", 
       RowBox[{"loadVMECQuantity", "[", 
        RowBox[{"directory", ",", "\"\<zmns\>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Total", "[", 
       RowBox[{
        RowBox[{"rmnc", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "*", 
        RowBox[{"Sin", "[", 
         RowBox[{
          RowBox[{"xm", "*", "0"}], "-", 
          RowBox[{"xn", "*", "0"}]}], "]"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]}], "Input",
