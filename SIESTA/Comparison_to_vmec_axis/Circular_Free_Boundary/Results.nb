06e33f97f073"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"sortedDirectories", "[", "parent_", "]"}], ":=", 
   RowBox[{"Sort", "[", 
    RowBox[{
     RowBox[{"FileNames", "[", 
      RowBox[{"\"\<*_*.*\>\"", ",", "parent"}], "]"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"ToExpression", "[", 
        RowBox[{"Last", "[", 
         RowBox[{"StringSplit", "[", 
          RowBox[{"#1", ",", "\"\<_\>\""}], "]"}], "]"}], "]"}], "<", 
       RowBox[{"ToExpression", "[", 
        RowBox[{"Last", "[", 
         RowBox[{"StringSplit", "[", 
